{
  "extension": {
    "version": "1.1",
    "active": true,
    "assets": {
      "icons": {
        "enabled": "../assets/icons/sdc-48.png",
        "disabled": "'../assets/icons/sdc-off-48.png'"
      }
    }
  },

  "useFakeResults": false,

  "search_engines": {
    "sdc": {
      "name": "sdc",
      "requestsDelay": 500,
      "api": {
        "trusted": "https://sourcesdeconfiance.org/api/trusted",
        "version": "https://sourcesdeconfiance.org/api/version"
      }
    },

    "google": {
      "name": "google",
      "maxResults": 200,
      "maxResultsPerRequest": 100,
      "requestsDelay": 2000
    },

    "paris": {
      "name": "paris",
      "maxResults": 200,
      "maxResultsPerRequest": 20,
      "resultsPerPage": 20,
      "requestsDelay": 1500
    },

    "edutheque": {
      "name": "edutheque",
      "maxResults": 200,
      "maxResultsPerRequest": 20,
      "resultsPerPage": 20,
      "requestsDelay": 1500
    }
  },

  "components": {
    "confiance": {
      "resultsPerPage": 10,
      "maxResults": 200
    }
  },

  "widgets": {
    "se_toolbar": {
      "title": "Sources de Confiance",
      "active": false
    },

    "navbar": {
      "main": {
        "component": "Main",
        "title": "Accueil",
        "status": "inactive"
      },

      "education": {
        "component": "Education",
        "title": "Education",
        "status": "active"
      }
    }
  }
}
